algoritmo "pgm13"
// Função : Armazenar dois valores e exibi a sua exponenciação
// Autor : Lucas Igor A M Nogueira
// Data : 21/01/2021

var

   num1: real

inicio

      //atribuir valor na variavel
      num1 <- 6^2
      

      //exibi o resultado das operaçãoes realizadas
      escreva("A exponenciação do numero é: ",num1)



fimalgoritmo
